{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "build.js$",
                "cmd": "node build.js"
            },
            {
                "match": "(pages|notes)(\\\\|/).*\\.md$",
                "cmd": "node build.js"
            },
            {
                "match": "(templates)(\\\\|/).*\\.(html|css|json)$",
                "cmd": "node build.js"
            }
        ]
    },
    "cSpell.words": [
        "linkify",
        "mathbb",
        "mathbf",
        "nanolett",
        "operatorname",
        "texmath"
    ]
}
